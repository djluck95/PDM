<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Darius.myapplication">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_healthpoint"
        android:label="store Point"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.Darius.application.Authentication.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Darius.application.Authentication.RegisterActivity" />
        <activity
            android:name="com.example.Darius.application.ClientController.MainActivity"
            android:label="store Point"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>